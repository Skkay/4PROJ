{% extends 'base.html.twig' %}

{% block title %}Hello GenderController!{% endblock %}

{% block body %}
<h1>Gender</h1>

<table class="table table-sm table-hover">
    <thead>
        <tr>
            <th scope="col">gender.id</th>
            <th scope="col">gender.label</th>
        </tr>
    </thead>
    <tbody>
        {% for gender in genders %}
        <tr>
            <td>{{ gender.id }}</td>
            <td>{{ gender.label }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}
